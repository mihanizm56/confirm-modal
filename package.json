{
  "name": "@wildberries/confirm-modal-portal",
  "version": "0.0.1-beta.1",
  "description": "confirm modal module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:watch": "jest --watch",
    "test": "set CI=true && jest",
    "test:coverage": "jest --coverage",
    "alias-resolve": "tscpaths -p tsconfig.json -s ./lib -o ./dist",
    "compile": "rm -fr ./dist && tsc --project tsconfig.json",
    "lint": "npx eslint . --ext .ts,.tsx --fix",
    "commit": "git add . && git commit --no-edit",
    "packages-vulnerability-check": "check-audit",
    "release": "npm run commit && git push && npm publish --access=public",
    "beta": "npm run commit && git push && npm publish --tag beta --access=public",
    "size": "pkgfiles",
    "build": "npm run compile && npm run alias-resolve"
  },
  "author": "mihanizm56",
  "license": "MIT",
  "homepage": "https://github.com/mihanizm56/notifications#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mihanizm56/notifications.git"
  },
  "bugs": {
    "url": "https://github.com/mihanizm56/notifications/issues"
  },
  "dependencies": {
    "@wildberries/notifications": "0.0.10-beta.8",
    "@wildberries/ui-kit": "0.0.1-beta.43",
    "redux-batched-actions": "0.4.1",
    "react-final-form": "6.4.0",
    "final-form": "4.19.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@mihanizm56/fetch-api": "3.1.2",
    "@types/classnames": "2.2.10",
    "@types/jest": "24.9.1",
    "@types/lodash-es": "4.17.3",
    "@types/react": "16.9.11",
    "@types/react-redux": "7.1.7",
    "@types/reselect": "2.2.0",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "commitizen": "4.0.4",
    "cross-env": "7.0.2",
    "cz-customizable": "6.2.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "3.0.9",
    "jest": "25.1.0",
    "npm-audit-resolver": "2.1.0",
    "pkgfiles": "2.3.2",
    "prettier": "1.18.2",
    "ts-jest": "25.4.0",
    "tscpaths": "0.0.9",
    "typescript": "3.8.3"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  }
}